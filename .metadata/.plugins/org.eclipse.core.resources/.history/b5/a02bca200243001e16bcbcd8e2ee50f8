package day08;

public class Ex01 {

	public static void main(String[] args) {
		int a[] = {100, 200};
		int b = 200;
		add(a);
		System.out.println(a[0]);
		System.out.println(a[1]);
		add2(b);
		System.out.println(b);
	}
	public static void add(int a[]) {
		a[a.length-1] = 300;
	}
	public static void add2(int b) {
		b=500;
		//리턴을 통해서 값을 변경.
	}

}
